@model ASPNETAOP.Models.AdminLocationEdit

@{
    ViewData["Title"] = "LocationEdit";
}

<h1>Location Edit</h1>

<hr />
<div class="row">
    <div class="col-md-9">
        <form asp-action="LocationEdit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="name" class="control-label"></label>
                <input asp-for="name" class="form-control" type="text">
                <span asp-validation-for="name" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="newName" class="control-label"></label>
                <input asp-for="newName" class="form-control" type="text">
                <span asp-validation-for="newName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="country" class="control-label"></label>
                <input asp-for="country" class="form-control" type="text">
                <span asp-validation-for="country" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="city" class="control-label"></label>
                <input asp-for="city" class="form-control" type="text">
                <span asp-validation-for="city" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="geographic_latitude" class="control-label"></label>
                <input asp-for="geographic_latitude" class="form-control" type="text">
                <span asp-validation-for="geographic_latitude" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="geographic_longitude" class="control-label"></label>
                <input asp-for="geographic_longitude" class="form-control" type="text">
                <span asp-validation-for="geographic_longitude" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="airborne" class="control-label"></label>
                <input asp-for="airborne" class="form-control" type="text">
                <span asp-validation-for="airborne" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="LocationEdit" class="btn btn-primary" />
            </div>
            <b>@ViewData["Message"]</b>
        </form>
    </div>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
