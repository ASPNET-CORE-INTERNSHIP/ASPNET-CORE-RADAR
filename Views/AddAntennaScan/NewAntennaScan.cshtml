@model List<ASPNETAOP.Models.AddAntenna>

@{
    ViewData["Title"] = "NewAntennaScan";
}

<h1>Build Relationship</h1>

<hr />
<div class="row">
    <div class="col-md-9">
        <form asp-action="NewAntennaScan">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div>
                @foreach (var antenna in Model)
                {
                    <div>
                        <h1>@antenna.name</h1>
                    </div>
                    <div>
                        <h1>@antenna.duty</h1>
                    </div>}
            </div>
            <div class="form-group">
                <input type="submit" value="NewAntennaScan" class="btn btn-primary" />
            </div>
            <b>@ViewData["Message"]</b>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Turn back</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
