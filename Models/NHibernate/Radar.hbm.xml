<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true" assembly="ASPNETAOP" namespace="ASPNETAOP.Models">
	<class name="Radar" table="Radar" dynamic-update="true" >
		<cache usage="read-write"/>
		<id name="ID" column="ID" type="Guid">
			<generator class="native" />
		</id>
		<property name="name" />
		<property name="system" />
		<property name="configuration" />
		<property name="transmitter_id" />
		<property name="receiver_id" />
		<property name="location_id" />
	</class>
</hibernate-mapping>

