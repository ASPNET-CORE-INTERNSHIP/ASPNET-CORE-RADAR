<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true" assembly="ASPNETAOP" namespace="ASPNETAOP.Models">
	<class name="Transmitter" table="Transmitter" dynamic-update="true" >
		<cache usage="read-write"/>
		<id name="ID" column="ID" type="Guid">
			<generator class="native" />
		</id>
		<property name="name" />
		<property name="modulation_type" />
		<property name="max_frequency" />
		<property name="min_frequency" />
		<property name="power" />
	</class>
</hibernate-mapping>

