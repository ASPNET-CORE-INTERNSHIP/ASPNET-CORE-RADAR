<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true" assembly="ASPNETAOP" namespace="ASPNETAOP.Models">
	<class name="Location" table="Location" dynamic-update="true" >
		<cache usage="read-write"/>
		<id name="ID" column="ID" type="Guid">
			<generator class="native" />
		</id>
		<property name="name" />
		<property name="country" />
		<property name="city" />
		<property name="geographic_latitude" />
		<property name="geographic_longitude" />
		<property name="airborne" />
	</class>
</hibernate-mapping>
